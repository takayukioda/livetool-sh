fps=15
insize="640x480"
outsize="640x480"
grab_position=":0.0+150,150"
maxrate="432k"
bufsize="864k"
flash_version="flashver=FME/3.0\20(compatible;20FMSc/1.0)"

## Video
video_codec="libx264"
video_profile="high"
x264opts=
video_sync_method=-1
#libx264 AVOption
preset=

## Audio
audio_codec="libmp3lame"
audio_frequency=44100
audio_bitrate="96k"
audio_channel=2
audio_sync_sample_rate=2
noise_reduction=100
## rtmp url
rtmpfile="rtmp.default"
